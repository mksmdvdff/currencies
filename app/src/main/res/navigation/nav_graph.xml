<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/currencies">

    <fragment
        android:id="@+id/currencies"
        android:name="davydov.annamoney.ui.currency.fragment.CurrencyFragment">
        <action
            android:id="@+id/currencies_to_lottie"
            app:destination="@id/lottie"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>

    <fragment
        android:id="@+id/lottie"
        android:name="davydov.annamoney.ui.lottie.LottieFragment">
    </fragment>

</navigation>